<script>
  var tags=[]
  tags=fetch('/product/alltags').then((response)=>{
    response.json().then((data)=>{
      tags=data
    })
  })
 
  const searchBar=document.getElementById('searchBar')
  const searchList=document.getElementById('searchList')
  searchBar.addEventListener('keyup',(e)=>{
    const matchingResult=tags.filter((tag)=>{
      return tag.toLowerCase().includes(e.target.value.toLowerCase())
    })
    var h=''
    for(var i=0;i<matchingResult.length;i++)
    {if (i>5)
      break;
      h+=`<ul class="searchItem"><a onclick="document.getElementById('searchBar').value("${matchingResult[i]}"); return false;">${matchingResult[i]}</a></ul>`
    }
    
    searchList.innerHTML=h
    console.log(matchingResult)
  })
 
 
 
</script>
