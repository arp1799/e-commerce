    <%- include('partials/other_page_header'); -%>
	<body class="woocommerce">
        <div class="xs-breadcumb">
            <div class="container">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb">
                        <li class="breadcrumb-item"><a href="/"> Home</a></li>
                        <li class="breadcrumb-item active" aria-current="page">Wishlist</li>
                    </ol>
                </nav>
            </div>
        </div>


       <div class="shop_header mt-3"><h3>Wish List</h3></div>

        <div class="cart-area xs-section-padding">
            <div class="container">
               <div class="row">
        <div class="col-md-12">
            
            
            <form class="woocommerce-cart-form" >
                <div class="table-responsive">
                    <table class="shop_table">
                        <thead>
                            <tr>
                            <th class="product-remove">&nbsp;</th>
                            <th class="product-thumbnail">&nbsp;</th>
                            <th class="product-name">Product</th> 
                            <th class="product-price">Price</th>
                          
                            <th class="product-subtotal">Total</th>
                            
                            </tr>
                        </thead>  
                        
                        <tbody>
                            <%for(var i=0; i<wishlist.length;i++){%>
                            <tr class="woocommerce-cart-form__cart-item cart_item">
                                <td class="product-remove">
                                    <a href="/user/removewishlist/<%=wishlist[i]._id%>" class="remove" aria-label="Remove this item" data-product_id="1332" data-product_sku="">Ã—</a>						
                                </td>
                                <td class="product-thumbnail">
                                    <a href="#"><img src="<%=wishlist[i].productdetail.avatar[0]%>" style="width:142px;height:125px" class="attachment-woocommerce_thumbnail" alt=""></a>						
                                </td>
                                <td class="product-name" data-title="Product">
                                    <a href="/product/id/<%=wishlist[i]._id%>"><%=wishlist[i].productdetail.productname%></a>
                                    <dl class="variation">
                                        <dt class="variation-Vendor">Vendor:</dt>
                                        <dd class="variation-Vendor">
                                            <p>Marketo</p>
                                        </dd>
                                    </dl>
                                </td>
                                <td class="product-price" data-title="Price">
                                    <span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">$</span><%=wishlist[i].productdetail.price%></span>						
                                </td>
                                
                                <td class="product-subtotal" data-title="Total">
                                    <span class="woocommerce-Price-amount amount"><span class="woocommerce-Price-currencySymbol">$</span><%=wishlist[i].productdetail.price%></span>
                                </td>

                                <td>
                                    <button onclick="addcart(this.value)" class="btn btn-primary"   value="<%=wishlist[i]._id%>">Add To Cart</button>
                                </td>
                            </tr>
                            <%}%>
                            <script>
                                function addcart(val){
                                   fetch(`/user/cart/`+val).then((res)=>{
                                       res.json().then((result)=>{
                                           console.log(result)
                                       })
                                   })
                                }	
                            </script>
                        </tbody>
                    </table>
                </div>
            </form>
          </div>
          </div>
          </div>


          <%- include('partials/footer'); -%>
          <!-- js file start -->
          <script src="/assets/js/jquery-3.2.1.min.js"></script>
          <script src="/assets/js/jquery-ui.js"></script>
          <script src="/assets/js/modernizr.js"></script>
          <script src="/assets/js/plugins.js"></script>
          <script src="/assets/js/Popper.js"></script>
